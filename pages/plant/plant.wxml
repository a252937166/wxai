<view class="container">
  <view bindtap="uploads">
    <button class="up">拍照</button>
  </view>
  <image style="width: 200px; height: 200px; background-color: #eeeeee;" src="{{img}}"></image>
  <button style="margin-top: 20px;" type="warn" bindtap="changeinfo">{{info}}</button>
  <text class="user-names">{{names}}</text>
  <text class="user-probabilitys">{{scores}}</text>
</view>
