<view class="container">
  <view bindtap="uploads">
    <button class="up">拍照</button>
  </view>
  <image style="width: 200px; height: 200px; background-color: #eeeeee;" src="{{img}}"></image>
  <button style="margin-top: 20px;margin-bottom: 20px;" type="warn" bindtap="changeinfo">{{info}}</button>
  <text class="user-age">{{ages}}</text>
  <text class="user-beauty">{{beautys}}</text>
   <text class="user-remark">{{remark}}</text>
</view>
